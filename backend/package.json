{
	"name": "backend",
	"version": "1.0.0",
	"description": "Backend dla aplikacji Suit Creator",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc && npm run copy-templates",
		"copy-templates": "ncp templates dist/templates",
		"start": "node dist/backend/src/index.js",
		"dev": "ts-node-dev --respawn --transpile-only src/index.ts",
		"prisma:generate": "dotenv -f .env -- prisma generate",
		"prisma:migrate:dev": "npx --no-install prisma migrate dev",
		"prisma:migrate:prod": "dotenv -f .env.production -- prisma migrate deploy",
		"generate-data": "dotenv -f .env -- ts-node scripts/generate-companies-data.ts",
		"import-images:dev": "dotenv -f .env -- ts-node scripts/mass-import.ts",
		"import-images:docker": "ts-node --project tsconfig.json scripts/mass-import.ts",
		"prisma:reset": "npx --no-install prisma migrate reset --force",
		"import:api": "curl -X POST -H \"x-import-token: a1b2c3d4-e5f6-7890-1234-56789abcdef0\" http://localhost:3001/api/admin/import-data",
		"db:reset-and-import": "npm run prisma:reset && npm run prisma:migrate:dev && npm run import-images:dev"
	},
	"keywords": [
		"suit",
		"creator",
		"fashion",
		"customization"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^5.22.0",
		"bcrypt": "^5.1.1",
		"canvas": "^2.11.2",
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"express": "^4.21.2",
		"express-validator": "^7.2.1",
		"helmet": "^7.2.0",
		"jsonwebtoken": "^9.0.1",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.2",
		"resend": "^4.6.0",
		"uuid": "^11.1.0",
		"winston": "^3.17.0",
		"zod": "^3.25.63"
	},
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cors": "^2.8.19",
		"@types/express": "^4.17.23",
		"@types/jest": "^29.5.3",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/morgan": "^1.9.10",
		"@types/multer": "^1.4.7",
		"@types/node": "^20.19.0",
		"@types/supertest": "^2.0.12",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"cross-env": "^7.0.3",
		"dotenv-cli": "^7.4.2",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^5.0.0",
		"jest": "^29.6.2",
		"ncp": "^2.0.0",
		"prettier": "^3.0.0",
		"prisma": "^5.22.0",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.8.3"
	}
}
